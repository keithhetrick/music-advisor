{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "RunSummary",
  "type": "object",
  "required": [
    "audio_hash",
    "pipeline_version",
    "config_fingerprint",
    "sidecar_status",
    "tempo_backend_detail",
    "warnings",
    "timestamps"
  ],
  "properties": {
    "audio_hash": { "type": "string" },
    "pipeline_version": { "type": "string" },
    "config_fingerprint": { "type": "string" },
    "sidecar_status": { "type": "string" },
    "tempo_backend_detail": { "type": "string" },
    "warnings": { "type": "array", "items": { "type": "string" } },
    "timestamps": {
      "type": "object",
      "required": ["started_at", "finished_at"],
      "properties": {
        "started_at": { "type": "string" },
        "finished_at": { "type": "string" }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": true
}
