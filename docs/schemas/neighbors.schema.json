{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Neighbors",
  "type": "object",
  "required": ["neighbors", "meta"],
  "properties": {
    "neighbors": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": { "type": "string" },
          "artist": { "type": "string" },
          "score": { "type": "number" },
          "decade": { "type": "string" },
          "tier": { "type": "string" }
        },
        "additionalProperties": true
      }
    },
    "meta": {
      "type": "object",
      "properties": {
        "top_k": { "type": ["integer", "null"] },
        "tier_filter": {
          "type": ["array", "null"],
          "items": { "type": "string" }
        }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
